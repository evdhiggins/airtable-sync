{
  "name": "@evdhiggins/airtable-sync",
  "version": "0.2.3",
  "description": "A one-way sync from a local datasource to Airtable",
  "main": "./built/main.js",
  "types": "built/main.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:src",
    "build:src": "npx tsc -p .",
    "build:types": "npx ttw deploy",
    "publish:patch": "npm run build && npm version patch && npm publish",
    "test": "npx jest",
    "tests": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evdhiggins/airtable-sync.git"
  },
  "keywords": [
    "airtable",
    "sync"
  ],
  "author": "Evan Higgins <evdhiggins@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evdhiggins/airtable-sync/issues"
  },
  "homepage": "https://github.com/evdhiggins/airtable-sync#readme",
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.3",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "typescript-type-writer": "github:evdhiggins/typescript-type-writer"
  },
  "dependencies": {
    "airtable": "^0.6.0",
    "better-sqlite3": "^5.4.0",
    "pg-promise": "^8.7.4"
  }
}